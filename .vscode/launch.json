{
	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceRoot}\\bin\\Debug\\netcoreapp1.1\\collada-parser.dll",
			"args": ["crate"],
			"cwd": "${workspaceRoot}",
			"stopAtEntry": false,
			"externalConsole": false,
			"internalConsoleOptions": "openOnSessionStart"
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach",
			"processId": "${command.pickProcess}"
		}
	]
}